<?xml version="1.0"?>
<object name="TestContainer" meta_type="Dexterity FTI" i18n:domain="plone"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Test Container</property>
 <property name="description" i18n:translate=""></property>
 <property name="factory">TestContainer</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property name="klass">plone.dexterity.content.Container</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types"/>
 <property name="behaviors">
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element value="plone.app.dexterity.behaviors.metadata.IDublinCore"/>
  <element value="collective.classifiers.themes"/>
  <element value="collective.classifiers.categories"/>
 </property>
 <!-- A schema (or model or model_source) is required.
      We only need a basically empty schema. -->
 <property name="schema">zope.interface.Interface</property>
 <property name="model_source"></property>
</object>
